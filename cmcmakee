analog@analog:~ $ cd ~

rm -rf gcm gcm.tar.gz gcm-build

wget -O gcm.tar.gz https://github.com/gnuradio/gnuradio-cmake-modules/archive/refs/heads/master.tar.gz \

 || curl -L https://github.com/gnuradio/gnuradio-cmake-modules/archive/refs/heads/master.tar.gz -o gcm.tar.gz



mkdir gcm

tar -xzf gcm.tar.gz -C gcm --strip-components=1



cmake -S gcm -B gcm-build -DCMAKE_INSTALL_PREFIX=/usr/local

sudo cmake --install gcm-build



echo "== /usr/local/share/cmake/gnuradio =="

ls /usr/local/share/cmake/gnuradio | egrep 'Gr(MinReq|CompilerSettings|Pybind)'

